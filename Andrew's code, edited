def consolidate_cart(cart)
  item_hash = {}
  #COMMENTED OUT NEXT LINE
  #item_array = cart.keys
  cart.each do |element|
    element.each do |item, data|
      item_hash[item] = data
      #ADDED IF-ELSE-END
      if item_hash[item][:count]
        item_hash[item][:count] += 1
      else
        item_hash[item][:count]=1
      end
      #END OF ADDITIONS
    end
  end
  return item_hash
end
